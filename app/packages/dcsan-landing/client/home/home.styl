

.fixedImageBox {
  position fixed
  top 0px
  left 100px
  width 100%
  height 100%
  z-index 0
}

.homePage {
  position relative
  top 200px
  z-index 10
  
  .imageInset {
    position absolute
    left 250px
    top  50px
  }

  .contentBlock {
    border 1px solid #848d89
    margin 10px
    margin-left 80px
    padding 10px
    border-radius 5px
    background-color white
  }

}


basicBar()
  border: none
  background: #9ef0a3
  // color: 


.progBox {
  position: fixed;
  bottom: 0px;
  z-index: 200;
  width: 100%;
}

progress {
  width: 100%;
  display: block;
  /* Important Thing */
  -webkit-appearance: none;
  basicBar()
}

progress::-webkit-progress-value {
  basicBar()
}

progress::-webkit-progress-value {
  basicBar()
}



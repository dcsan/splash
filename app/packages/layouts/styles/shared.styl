
.headerTitle {
	font-family: 'Oswald', sans-serif;
	color: white;
	font-size: 30px;
}

.headerSubtitle {
	font-family: 'Mouse Memoirs', sans-serif;
	color: #ccc;
	font-size: 24px;
}


.announceBox {
	border: 5px solid black;
  background-color: white;
	text-align: left;
  padding 10px
}

.bigSelect {
  border 2px solid black
  border-radius 10px
  background-color white
  box-shadow 2px 2px 2px rgba(0,0,0, 0.5)
  color black
  padding 10px
  font-family: 'Oswald', sans-serif;
  font-size 24px
  line-height 60px
  cursor: pointer
  /*margin: 0px*/
}

.bigSelect:hover {
  border 2px solid #1c3f26
  background-color #c5ffc7
  box-shadow 2px 2px 4px rgba(0,0,0, 0.5)
  transform scale(1.01)
}

.announceHeading {
  font-family: 'Oswald', sans-serif;
  color: white;
  font-size: 40px;
  background-color: black;
  padding: 10px;
}

.announceMsg {
	font-family: 'Mouse Memoirs', sans-serif;
	color: black;
	font-size: 40px;
	background-color: white;
	border: 5px solid black;
	margin: 5%;
	padding: 20px;
	text-align: center;
}

.bigBox {
  border: 5px solid black;
  box-shadow: 4px 4px 10px rgba(0,0,0, 0.5);
  border-radius: 5px;
  margin: 5%;
  background-color: white;
  color: black;
  padding: 10px;
}


.bigButton {
	font-family: 'Oswald', sans-serif;
	color: black;
	font-size: 40px;
	background-color: #8995d7;
	bevelBorder(3, #8995d7)
	border-radius: 10px;
	box-shadow: 3px 10px 3px rgba(0,0,0, 0.5);
	margin: 10%;
	padding: 20px;
	text-align: center;
}



.sceneButton {
	position: fixed;
	box-shadow: 2px 4px 10px rgba(0, 0, 0, 0.5);
	border-radius: 50%;
	width: 60px;
	height: 60px;
	background-size: contain;
	background-repeat: no-repeat;
	cursor: pointer;
	z-index: 300;
	// anim(bounceIn 1s both)
}



.rightEditBox {
  position: absolute
  right: 10px
}

// holds next and prev buttons
.bottomNavBar {
  position: fixed;
  bottom: 0px;
  width: 100%;
  // border-top: 1px solid #666;
  padding: 10px;
  // padding-top: 10px;
  // padding-bottom: 10px;
  font-size: 60px;
  height: 80px;
  z-index: 500;
  background-color: rgba( 255,255, 255, 0.5)

  .btn {
    position: absolute;
    bottom: 0px;
    text-align: center;
    cursor: pointer;
    width: 16.66%
    border: 1px solid gray;
    background-color rgba(255,255,255, 0.7)
  }

  .btn0 {
    left: 0
  }

  .btn1 {
    left: 16.6%;
  }

  .btn2 {
    left: 33.3%;
  }

  .btn3 {
    left: 49.9%;
  }

  .btn4 {
    left: 66.5%;
  }

  .btn5 {
    right: 0
  }

  .nextBar {
    position: absolute;
    right: 10px;
    bottom: 0px;
    text-align: right;
    cursor: pointer;
  }

  .backBar {
    position: absolute;
    left: 10px;
    bottom: 0px;
    text-align: left;
    cursor: pointer;
  }

  .navButtonLabel {
    color: #222;
    text-align: inherit;
    width: 100%;
    font-size: 60px;
    font-family: 'Oswald', sans-serif;
  }

  .navButtonText {
    color: gray;
    font-size: 12px;
  }

}

.sideMenuOuter {
  position: fixed;
  top: 0px;
  left: 0px;
  z-index: 1000;
  // width: 300px;
  display: none;  // startup state
  opacity: 1;
}

.sideMenuBack {
  position: fixed;
  top: 0px;
  left: 0px;
  height: 120%;
  width: 120%;
  background-color: rgba(0,0,0, 0.7)
  z-index: 400;
}

.sideMenuInner {
  position: relative;
  top: 50px;
}

.sideItem {
  position: relative;
  left: -5px;
  border: 5px solid black;
  border-radius: 2px;
  background-color: white;
  padding: 5px;
  padding-left: 30px;
  padding-right: 10px;
  font-family: 'Oswald', sans-serif;
  color: black;
  font-size: 40px;
  line-height: 75px;
  cursor: pointer;
  z-index: 550;
  display: inline;
}
